  "related_applications": [
    {
      "platform": "web",
      "url": "https://vendionline.eu/",
      "id": "vendionline-eu"
    }
  ],
  
  "prefer_related_applications": false,
  
  "protocol_handlers": [
    {
      "protocol": "web+aitrainer",
      "url": "/?action=%s"
    }
  ],
  
  "edge_side_panel": {
    "preferred_width": 400
  },
  
  "launch_handler": {
    "client_mode": "navigate-existing"
  },
  
  "handle_links": "preferred",
  
  "file_handlers": [
    {
      "action": "/upload",
      "accept": {
        "image/*": [".png", ".jpg", ".jpeg", ".webp"],
        "text/html": [".html", ".htm"]
      },
      "icons": [
        {
          "src": "data:image/svg+xml,<svg xmlns='http://www.w3.org/2000/svg' viewBox='0 0 24 24' fill='%23667EEA'><path d='M14,2H6A2,2 0 0,0 4,4V20A2,2 0 0,0 6,22H18A2,2 0 0,0 20,20V8L14,2M18,20H6V4H13V9H18V20Z'/></svg>",
          "sizes": "96x96",
          "type": "image/svg+xml"
        }
      ],
      "launch_type": "single-client"
    }
  ],
  
  "share_target": {
    "action": "/share",
    "method": "POST",
    "enctype": "multipart/form-data",
    "params": {
      "title": "title",
      "text": "text",
      "url": "url",
      "files": [
        {
          "name": "files",
          "accept": ["image/*", "text/html"]
        }
      ]
    }
  },
  
  "scope_extensions": [
    {
      "origin": "https://ai-trainer-production-8fd9.up.railway.app"
    }
  ],
  
  "permissions": [
    "storage",
    "background-sync",
    "push"
  ],
  
  "features": [
    "Cross Platform",
    "Fast Performance", 
    "Background Sync",
    "Offline Ready",
    "Push Notifications",
    "File System Access",
    "Share Target",
    "Protocol Handler"
  ]
}
